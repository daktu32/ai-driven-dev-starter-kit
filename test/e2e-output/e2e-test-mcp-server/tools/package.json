{
  "name": "e2e-test-mcp-server-dev-tools",
  "version": "1.0.0",
  "description": "Development tools and configuration for e2e-test-mcp-server",
  "private": true,
  "scripts": {
    "lint": "eslint ../src --ext .ts,.tsx --fix",
    "lint:check": "eslint ../src --ext .ts,.tsx",
    "format": "prettier --write '../src/**/*.{ts,tsx,json,md}'",
    "format:check": "prettier --check '../src/**/*.{ts,tsx,json,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "quality-check": "npm run lint:check && npm run format:check && npm run type-check",
    "pre-commit": "npm run quality-check && npm run test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}